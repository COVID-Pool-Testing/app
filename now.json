{
    "version": 2,
    "name": "github-action-cra",
    "alias": "github-action-cra",
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "storybook-static"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "cache-control": "s-maxage=0"
            },
            "dest": "/index.html"
        }
    ]
}